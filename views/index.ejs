        <div class="swiper-text-box swiper-text-box1 swiper-index-box">
          <div class="swiper-text-read1 d1 swiper-animation"><%= banner.title %></div>
          <div class="swiper-text-read2 d2 en swiper-animation"><%= banner.titleEN %></div>
          <div class="swiper-text-read3 d3 swiper-animation"><%= banner.subTitle %></div>
          <!-- <div class="swiper-text-read4 d4 en swiper-animation clear">
            <a href="<%= banner.casesLink %>">
              <i class="iconfont icon-shipin"></i>
              <span><%= banner.casesTitle %></span>
            </a>
          </div> -->
        </div>
        <div class="swiper-text-bottom d5">
          <div class="swiper-text-i"></div>
        </div>
      </div>
    </div>
    <!-- Add Pagination -->
    <!--<div class="swiper-pagination"></div>-->
  </div>
</section>
<!--我们的服务-->
<section class="services-wrap index-padding-wrap">
  <div class="width-auto">
    <div class="index-title">
      <span class="en wow fadeInUp d1"><%= our.title %></span>
      <span class="wow fadeInUp d2"><%= our.subTitle %></span>
    </div>
    <div class="services-box clear">
      <% our.blockList.forEach(item => { %>
        <div class="services-list wow fadeInUp">
          <a href="<%= item.link %>">
            <span class="iconfont icon-<%= item.icon %>"></span>
            <span><%= item.name %></span>
            <span class="en"><%= item.desc %></span>
          </a>
        </div>
      <% })%>
    </div>
  </div>
</section>
<!--案例-->
<section class="case-wrap index-padding-wrap">
  <div class="width-auto">
    <div class="index-title">
      <span class="en wow fadeInUp d1"><%= product.title %></span>
      <span class="wow fadeInUp d2"><%= product.subTitle %></span>
    </div>
    <div class="case-box">
      <div class="case-ul clear">
        <% product.product.forEach(item => { %>
          <div class="case-li wow fadeInUp">
            <a
              href="javascript:void(0);"
              class="alivideoItemLnk"
              data-videoId="2687e6d8201a4324842c665abedd59f5"
              data-videoTitle="三维打印机CR-6 SE0504.mp4"
              data-videoCover="http://video.wavesz.cn/2687e6d8201a4324842c665abedd59f5/snapshots/4362cb08081e4d1dbde832cd9f7c9937-00005.jpg">
              <div class="case-img data3"
                data-src="<%= item.url %>">
                <div class="case-icon iconfont icon-shipin"></div>
              </div>
              <div class="case-title-wrap">
                <div class="case-title space-over"><%= item.title %></div>
                <div class="case-lable space-over">
                  <% item.tag.forEach(t => {%>
                    <span><%= t %></span>
                  <%}) %>
                </div>
              </div>
            </a>
            <div class="case-read-button">
              <a href="/cases/detail.html">详情&gt;</a>
            </div>
          </div>
        <% })%>
      </div>
      <div class="index-more-button wow fadeInUp d1">
        <a href="/cases.html">
          <span>MORE</span>
        </a>
      </div>
    </div>
  </div>
</section>
