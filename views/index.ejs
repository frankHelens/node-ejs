<div class="prism-player-wrap" id="prism-player-wrap">
  <div class="inDiv">
    <div class="closeBtn closeBtn1"></div>
    <div class="prism-player" id="J_prismPlayer"></div>
  </div>
  <div class="closeBtn closeBtn2"></div>
</div>
<script>
  $(function () {
    var _win = $(window), _wrap = $('#prism-player-wrap'), _closeBtn = _wrap.find('.closeBtn'), _playId = $('#J_prismPlayer'), _in = _wrap.find('.inDiv').eq(0), _bdy = $(document.body), _player;
    function setPrismInDivSize() {

    }

    setPrismInDivSize();
    _win.resize(setPrismInDivSize);

    function ali_eve(e) {
      var _this = $(this);
      var _videoId = _this.attr('data-videoid'), _videoCover = _this.attr('data-videocover'), _videoAuth = '';
      $.ajax({
        type: 'POST',
        async: true,
        cache: false,
        url: "/Index/getVideoAuth",
        dataType: 'json',
        data: {
          videoid: _videoId
        },
        timeout: 600000,
        success: function (data) {
          if (data && data.videoAuth) {
            _videoAuth = data.videoAuth;

            _wrap.fadeIn(f);
            $('.inDiv').addClass('inDivTop');
          } else {
            alert('播放出错');
          }

          function f() {
            _player = new Aliplayer({
              id: 'J_prismPlayer',
              width: '100%',
              height: '100%',
              autoplay: false,
              isLive: false,
              rePlay: false,
              vid: _videoId,
              playauth: _videoAuth,
              cover: _videoCover,
              playsinline: true,
              preload: true,
              useH5Prism: true,
              mediaType: 'video',
              region: 'cn-shanghai'
            });
          }

        },
        error: function (e) {
          console.log(e.status);
          console.log(e.responseText);
        }
      });


    }

    _bdy.on('click', '.alivideoItemLnk', ali_eve);
    _closeBtn.click(function () {
      $('.inDiv').removeClass('inDivTop');
      _wrap.stop(true, true).fadeOut();
      _player.dispose();
      _player = null;
    });

  });


</script>
